<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/goodsTextView2"
        android:background="#E9C8C5"
        android:layout_width="245dp"
        android:layout_height="40dp"
        android:layout_marginStart="1dp"
        android:textColor="#000000"
        android:maxLines="1"
        android:fontFamily="sans-serif-condensed-medium"
        android:gravity="start|center_vertical"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/insertedFactTextView2"
        android:background="#B6D3B8"
        android:layout_width="24dp"
        android:layout_marginStart="1dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:maxLines="1"
        android:gravity="center|center_vertical"
        android:fontFamily="sans-serif-condensed-medium"
        android:textSize="16sp"
        android:layout_toEndOf="@+id/goodsTextView2" />

    <TextView
        android:id="@+id/time"
        android:background="#E9C8C5"
        android:layout_width="84dp"
        android:layout_marginStart="1dp"
        android:layout_height="40dp"
        android:textColor="#000000"
        android:maxLines="1"
        android:gravity="center|center_vertical"
        android:fontFamily="sans-serif-condensed-medium"
        android:textSize="15sp"
        android:layout_toEndOf="@+id/insertedFactTextView2" />
</RelativeLayout>